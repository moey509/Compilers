# Test for memory leaks

interface Color(String color){
	fun getColor() : String = color;
}

class Black(){
	super("Black");
	r := 256;
	g := 256;
	b := 256;
	s := getColor();
	
	fun getColor() : String = s++"!";
}


fun foo(int : Integer) : Iterable<String> {
	if (int <= 0) {
		return (Black().getColor());
	}
	else
		return foo(int-1)++foo(int-1);
}



x = foo(20);
x = foo(18);
